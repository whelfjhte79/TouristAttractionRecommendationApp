<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/reservation_prev_img"
                android:layout_width="53dp"
                android:layout_height="59dp"
                android:layout_marginStart="25dp"
                android:layout_marginTop="82dp"
                android:layout_marginEnd="19dp"
                android:layout_marginBottom="54dp"
                app:layout_constraintBottom_toTopOf="@+id/reservation_bg1"
                app:layout_constraintEnd_toStartOf="@+id/reservation_content_view"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:src="@drawable/search_page_prev_button" />

            <TextView
                android:id="@+id/reservation_content_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="19dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="220dp"
                android:layout_marginBottom="90dp"
                android:text="예약 내역"
                android:textSize="24sp"
                app:layout_constraintBottom_toTopOf="@+id/reservation_bg1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/reservation_prev_img"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/reservation_bg1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="36dp"
                android:layout_marginTop="30dp"
                android:layout_marginEnd="37dp"
                android:layout_marginBottom="29dp"
                android:src="@drawable/reservation_bg"
                app:layout_constraintBottom_toTopOf="@+id/reservation_bg2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/reservation_prev_img" />

            <ImageView
                android:id="@+id/reservation_bg2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="36dp"
                android:layout_marginTop="28dp"
                android:layout_marginEnd="37dp"
                android:layout_marginBottom="70dp"
                android:src="@drawable/reservation_bg"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/reservation_bg1" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/ConstraintLayout2"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/reservation_prev_button"
                android:layout_width="73dp"
                android:layout_height="79dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="30dp"
                android:layout_marginEnd="322dp"
                android:background="#00ff0000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/reservation_date_output"
                android:layout_width="202dp"
                android:layout_height="48dp"
                android:layout_marginStart="25dp"
                android:layout_marginTop="31dp"
                android:layout_marginEnd="62dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/reservation_date_view"
                app:layout_constraintTop_toBottomOf="@+id/reservation_page_attraction_name" />

            <TextView
                android:id="@+id/reservation_name_output"
                android:layout_width="137dp"
                android:layout_height="47dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="29dp"
                android:layout_marginEnd="143dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/reservation_name"
                app:layout_constraintTop_toBottomOf="@+id/reservation_date_output" />

            <TextView
                android:id="@+id/reservation_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="63dp"
                android:layout_marginTop="49dp"
                android:layout_marginEnd="12dp"
                android:text="예약자명:"
                app:layout_constraintEnd_toStartOf="@+id/reservation_name_output"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/reservation_date_view" />

            <TextView
                android:id="@+id/reservation_date_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="59dp"
                android:layout_marginTop="34dp"
                android:layout_marginEnd="25dp"
                android:text="예약 날짜:"
                app:layout_constraintEnd_toStartOf="@+id/reservation_date_output"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/reservation_page_attraction_name" />

            <TextView
                android:id="@+id/reservation_page_attraction_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="69dp"
                android:layout_marginTop="63dp"
                android:layout_marginEnd="264dp"
                android:text="예약 장소명:"
                android:textColor="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/reservation_prev_button" />

            <Button
                android:id="@+id/reservation_review_upload_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="108dp"
                android:layout_marginTop="162dp"
                android:layout_marginEnd="70dp"
                android:background="#00ff0000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/reservation_page_attraction_name"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </FrameLayout>

</LinearLayout>