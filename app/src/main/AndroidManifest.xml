<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.touristattraction">
    <uses-permission android:name="android.permission.INTERNET" />
    android:usesCleartextTraffic="true">
    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TouristAttraction">
        <activity android:name=".InitScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpPage" />
        <activity android:name=".LoginPage" />
        <activity android:name=".ReviewPage" />
        <activity android:name=".MyPage" />
        <activity android:name=".MainScreen" />
        <activity android:name=".MyAttractionPage" />
        <activity android:name=".SearchPage" />
        <activity android:name=".SearchResultPage" />
        <activity android:name=".ReservationList" />
    </application>

</manifest>